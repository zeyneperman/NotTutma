<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App3.SQlite.Login">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout>
                <Label Text="Notes Login Page" HorizontalOptions="Center"></Label>
                <Editor x:Name="editor" Placeholder="Enter Note"></Editor>
                <Editor x:Name="editor1" Placeholder="Enter Note"></Editor>
            </StackLayout>
            <StackLayout>
                <Button Text="save" Clicked="Button_Clicked" Grid.Column="0"></Button>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>